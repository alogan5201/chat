    "@capacitor/ios": "^3.3.1",
    "@capacitor/keyboard": "^1.1.3",
    "@capacitor/splash-screen": "^1.1.6",
    "@capacitor/status-bar": "^1.0.6",
    "@reduxjs/toolkit": "^1.6.2",
    "dom7": "^3.0.0",
    "firebase": "^9.3.0",
    "framework7": "^6.3.7",
    "framework7-icons": "^5.0.5",
    "framework7-react": "^6.3.7",
    "haversine-geolocation": "^1.6.0",
